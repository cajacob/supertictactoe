<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.supertictactoe.supertictactoe.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/supertictactoe/greet</url-pattern>
  </servlet-mapping>
  	<servlet>
		<servlet-name>Projectpage</servlet-name>
		<servlet-class>projectpage.ProjectpageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Projectpage</servlet-name>
		<url-pattern>/projectpage</url-pattern>
	</servlet-mapping>
  <!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>home.jsp</welcome-file>
	</welcome-file-list>
</web-app>
